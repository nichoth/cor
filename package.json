{
  "name": "cor",
  "version": "0.0.0",
  "description": "cor",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && concurrently --kill-others \"npx eleventy --serve\" \"npm run sass-watch\" \"npm run watch-js\"",
    "build": "mkdir -p public && mkdir -p public/admin && npm run sass && npm run build-js && eleventy",
    "build-js": "browserify --debug -p esmify src/index.js > public/bundle.js ",
    "watch-js": "watchify --debug -p esmify src/index.js -o public/bundle.js ",
    "sass": "sass ./src/style/style.scss ./public/style.css",
    "sass-watch": "sass --watch src/style:public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nichoth/cor.git"
  },
  "author": "Nick Thomas <nichoth@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nichoth/cor/issues"
  },
  "homepage": "https://github.com/nichoth/cor#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "browserify": "^16.5.2",
    "budo": "^11.6.4",
    "concurrently": "^6.0.2",
    "esmify": "^2.1.1",
    "sass": "^1.32.10"
  }
}
